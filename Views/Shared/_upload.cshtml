@model Meeting_Minutes.Models.FileUpd



<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Upload Files</h5>
        <section>
            <form method="post" enctype="multipart/form-data" asp-controller="MeetingItems" asp-action="UploadFile">
                <div class="form-group">
                    <div class="col-md-10">
                        <input asp-for="@Model.FileList" type="file" multiple />
                        <button type="submit" class="btn btn-primary">Submit File</button>
                    </div>
                </div>
            </form>
        </section>
    </div>
</div>